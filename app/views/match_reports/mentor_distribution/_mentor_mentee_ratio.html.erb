<div class="clearfix">
  <h4 class="m-b-md pull-left col-sm-12 p-l-0">
    <%= "feature.match_report.content.mentor_mentee_ratio".translate(Mentee: _Mentee, Mentor: _Mentor) %>
  </h4>
  <div class="col-xs-12 m-b-md no-padding">
    <div class="col-xs-5 no-padding clearfix" id="mentee_ratio">
      <div class="clearfix">
        <div class="text-navy m-t-sm pull-left"><%= get_icon_content("fa fa-users fa-lg fa-2x") %></div>
        <div class="font-30 pull-left m-r-sm"><%= mentee_view_users.count %></div>
        <div class="pull-left m-t-sm"><%= mentee_view.title %></div>
      </div>
    </div>
    <div class="h1 col-xs-1 font-300 no-padding m-t-xs clearfix">
      <%= ":" %>
    </div>
    <div class="col-xs-5 no-padding clearfix" id="mentor_ratio">
      <div class="clearfix">
        <div class="text-warning m-t-sm pull-left"><%= get_icon_content("fa fa-users fa-lg fa-2x") %></div>
        <div class="font-30 pull-left m-r-sm"><%= mentor_view_users.count %></div>
        <div class="pull-left m-t-sm"><%= mentor_view.title %></div>
      </div>
    </div>
  </div>
</div>