<div class="col-lg-4 col-md-4 col-sm-6 col-xs-12" id="catogery_<%= catogory[:catogory] %>">
  <div class="m-r-md m-b-md word_break">
    <div class="ibox">
      <div class="ibox-content">
        <div class="cjs-same-heignt">
          <h3 class="text-center m-b-md">
            <%= link_to catogory[:name], category_mails_mailer_templates_path(category: catogory[:catogory]) %>
          </h3>
          <h1 class="text-center">
            <i class="fa fa-envelope fa-fw"></i>
            <%= link_to catogory[:count], category_mails_mailer_templates_path(category: catogory[:catogory]), class: "subcatogories_link" %>
          </h1>
          <hr/>
          <div>
            <%= catogory[:description] %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>