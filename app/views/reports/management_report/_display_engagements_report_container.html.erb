<div class="col-md-6 cjs-tile-container cjs_tile_<%= DashboardReportSubSection::Tile::ENGAGEMENTS %>" id="engagements_info_container">
  <%= render(partial: "reports/management_report/report_tile_settings", locals: {tile: DashboardReportSubSection::Tile::ENGAGEMENTS}) %>
  <% engagement_type = program.get_engagement_type %>
  <h3 class="font-noraml"><%= engagement_type == Program::Dashboard::EngagementsReport::MEETINGS_ENGAGEMENT_TYPE ? "feature.reports.content.successful_completed_meetings".translate(meetings: _meetings) : "feature.reports.content.healthy_connections_in_time_period".translate(mentoring_connections: _mentoring_connections) %></h3>
  <div class="ibox" id="engagements_info">
    <%= render(partial: "common/tile_placeholder") %>
  </div>
</div>