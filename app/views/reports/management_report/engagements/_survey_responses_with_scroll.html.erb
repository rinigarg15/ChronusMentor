<% survey_responses.each do |answer| %>
  <% data_attribute = current_program.only_one_time_mentoring_enabled? ? {url: survey_response_meeting_path(answer.member_meeting.meeting, get_data_hash_for_survey_response_link(answer))} : {url: survey_response_group_path(answer.group, get_data_hash_for_survey_response_link(answer))} %>
  <%= display_survey_response_link(answer, data_attribute, true) %>
<% end %>
<% if next_page_index %>
  <div id="infinite-scrolling">
    <%= link_to("feature.reports.content.scroll_survey_responses".translate, "javascript:void(0)", {data: { url: scroll_survey_responses_dashboard_report_sub_sections_path}, class: "cjs_infinite_scrolling_link hide"}) %>
    <div class="sk-spinner sk-spinner-circle hide">
      <div class="sk-circle1 sk-circle"></div>
      <div class="sk-circle2 sk-circle"></div>
      <div class="sk-circle3 sk-circle"></div>
      <div class="sk-circle4 sk-circle"></div>
      <div class="sk-circle5 sk-circle"></div>
      <div class="sk-circle6 sk-circle"></div>
      <div class="sk-circle7 sk-circle"></div>
      <div class="sk-circle8 sk-circle"></div>
      <div class="sk-circle9 sk-circle"></div>
      <div class="sk-circle10 sk-circle"></div>
      <div class="sk-circle11 sk-circle"></div>
      <div class="sk-circle12 sk-circle"></div>
    </div>
  </div>
<% end %>