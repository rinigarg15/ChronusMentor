<div class="row-fluid" ng-cloak>
  <div class="section-1-container">
    <div class="column-1">
      <div class="box-1">
        <%= render partial: "reports/outcomes_report/detailed_connection_report/section_one_box", locals: {scope: 'sectionOneData.overall', connection_title: "feature.outcomes_report.title.total_connections".translate(:"Mentoring_Connections" => _Mentoring_Connections), user_title: "feature.outcomes_report.title.users_total_connections".translate, is_column_1: true} %>
      </div>
    </div>
    <div class="column-2">
      <div class="help-text"> <%= "feature.outcomes_report.content.detailed_connection_report_breakdown_text".translate(Mentoring_Connections: _mentoring_connections) %></div>

    </div>
    <div class="column-3">
      <div class="box-1">
        <%= render partial: "reports/outcomes_report/detailed_connection_report/section_one_box", locals: {scope: 'sectionOneData.ongoing', connection_title: "feature.outcomes_report.content.ongoing_connections_label".translate(Mentoring_Connections: _Mentoring_Connections), user_title: "feature.outcomes_report.content.total_users".translate} %>
      </div>

      <div class="box-2">
        <%= render partial: "reports/outcomes_report/detailed_connection_report/section_one_box", locals: {scope: 'sectionOneData.dropped', connection_title: "feature.outcomes_report.content.dropped_connections_label".translate(Mentoring_Connections: _Mentoring_Connections), user_title: "feature.outcomes_report.content.total_users".translate} %>
      </div>
      <div class="box-3">
        <%= render partial: "reports/outcomes_report/detailed_connection_report/section_one_box", locals: {scope: 'sectionOneData.completed', connection_title: "feature.outcomes_report.content.completed_connections_label".translate(Mentoring_Connections: _Mentoring_Connections), user_title: "feature.outcomes_report.content.total_users".translate} %>
      </div>

    </div>
    <div class="column-4">
      <div class="help-text">
        <%= "feature.outcomes_report.content.reported_positive_outcomes".translate %> <%= embed_icon(TOOLTIP_IMAGE_CLASS + " cjs-tool-tip", "", :data => {:desc => "feature.outcomes_report.tooltip.connection_report_positive_outcome_tooltip".translate}) %>
      </div>
    </div>
    <div class="column-5">
      <div class="box-1">
        <%= render partial: "reports/outcomes_report/detailed_connection_report/section_one_box", locals: {scope: 'sectionOneData.positive_outcomes', connection_title: "feature.outcomes_report.content.positive_outcome_connections_label".translate(Mentoring_Connections: _Mentoring_Connections), user_title: "feature.outcomes_report.content.total_users".translate} %>
      </div>
    </div>

  </div>

  <div class="row-fluid">
    <em class="pull-right small" ng-show="prevWindowValid"><%= "feature.outcomes_report.content.percent_change_label".translate(start_date: "{{prevRangeStartDateText}}", end_date: "{{prevRangeEndDateText}}") %></em>
  </div>
</div>