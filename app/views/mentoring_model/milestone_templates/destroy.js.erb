MilestoneTemplates.removeTemplate("<%= @milestone_template.id.to_s %>");
closeQtip();
<% @all_task_templates.each do |milestone_template_id, task_templates| %>
  <% content = render partial: "mentoring_model/task_templates/task_template", collection: task_templates %>
  MilestoneTemplates.refreshTasks("<%= milestone_template_id.to_s %>", "<%= j(content) %>");
<% end %>