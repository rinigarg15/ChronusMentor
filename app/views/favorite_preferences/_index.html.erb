<% if favorite_users.count.zero? %>
  <%= render partial: "favorite_preferences/zero_preference", locals: {src: src} %>
<% else %>
  <%= ibox nil, content_class: "no-padding" do %>
    <div id="cjs_favorite_listing">
    <%= listing_page(favorite_users, partial: 'users/mentor', collection_key: :mentor, locals: {profile_last_updated_at: profile_last_updated_at, show_availability_icons: true, show_favorite_ignore_links: true, skip_ignore_links: true, favorite_preferences_hash: favorite_preferences_hash, ignore_preferences_hash: ignore_preferences_hash, src: src} ) %>
    </div>
  <% end %>
<% end %>
<%= javascript_tag do %>
  jQuery(document).ready(function(){
    MentorRequests.showRequestConnectionPopup();
  });
  ShowAndHideToggle();
  initialize.initializeMatchDetailsPopup();
<% end %>