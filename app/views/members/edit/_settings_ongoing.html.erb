<%= form_for @profile_user, :remote => true, :url => update_settings_member_path(@profile_member), :html => {:class => 'form-horizontal', id: "ongoing_settings_#{@profile_member.id}"} do |f| %>
  <div>
    <div id="max_connections_limit" class="m-t-sm">
      <%= render_max_connections_limit(f, current_program, text_field_id: "user_max_connections_limit", from_member_profile: true) %>
    </div>
  </div>
  <% action_set do %>
    <div class="pull-right">
      <%= link_to('display_string.Cancel'.translate, "javascript:void(0)", :class => 'cjs-cancel-ibox btn btn-white') %>
      <%= f.submit "display_string.Save".translate, :id => "submit_ongoing_settings", :class => "btn btn-primary", data: {disable_with: "display_string.Please_Wait".translate} %>
    </div>
  <% end %>
<%end%>


