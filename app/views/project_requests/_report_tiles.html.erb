<%= render layout: 'common/report_tiles' do %>
  <%= render_report_tiles(project_request_hash[:received_count], "fa fa-user-plus", "feature.mentor_request.label.Received".translate, "") %>
  <%= render_report_tiles(project_request_hash[:pending_count], "fa fa-exclamation-triangle ", "feature.meeting_request.label.pending".translate, "") %>
  <%= render_report_tiles(project_request_hash[:accepted_count], "fa fa-check", "feature.meeting_request.label.accepted".translate, "") %>
  <%= render_report_tiles(project_request_hash[:others_count], "fa fa-close", "feature.admin_view.label.Others_html".translate(tooltip: embed_icon(TOOLTIP_IMAGE_CLASS,'', id: 'others_desc')),"" ) %>
  <%= tooltip("others_desc", "feature.project_request.content.requests_dropped".translate(mentoring_connection: _mentoring_connection)) %>
<% end %>