<% if @from_settings_page %>
  ChronusValidator.ErrorManager.ShowPageFlash(true,'<%= "flash_message.user_flash.mentoring_slot_deletion_success".translate %>');
  <% if @mentoring_slots.size == 0 %>
    jQuery('.cjs_slots_available_add_slot').hide();
    jQuery('.cjs_slots_unavailable_add_slot').show();
  <% end %>
  jQuery("#cjs_mentoring_slots_list").replaceWith('<%= j(render(:partial => "mentoring_slots/list_mentoring_slots", :locals => {:mentoring_slots => @mentoring_slots, :member => @profile_member})) %>');
<% else %>
  closeQtip();
  jQuery('#calendar').fullCalendar('refetchEvents');
<% end %>