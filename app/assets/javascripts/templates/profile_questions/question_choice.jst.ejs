<li class="draggable cjs_quicksearch_item list-group-item p-r-xs clearfix no-border" id="profile_question_<%= profile_question_id %>_<%= choice_id %>_container" data-choice-id="<%= choice_id %>">
  <div class="col-xs-11 p-l-0">
    <label for="profile_question_<%= profile_question_id %>_<%= choice_id %>_text" class="sr-only"> <%= choice_text || placeholder_text %> </label>
    <% if(disabled_for_editing){ %>
      <input type="text" name="profile_question[existing_question_choices_attributes][][<%= choice_id %>][text]" id="profile_question_<%= profile_question_id %>_<%= choice_id %>_text" placeholder="<%= placeholder_text %>" value="<%= choice_text %>" class="form-control input-sm" disabled/>
    <% }else{ %>
      <input type="text" name="profile_question[existing_question_choices_attributes][][<%= choice_id %>][text]" id="profile_question_<%= profile_question_id %>_<%= choice_id %>_text" placeholder="<%= placeholder_text %>" value="<%= choice_text %>" class="form-control input-sm"/>
    <% } %>
  </div>
  <div class="fa fa-plus-circle p-t-xs m-r-xs cjs_add_choice cursor-pointer" data-choice-id="<%= choice_id %>" data-profile-question-id="<%= profile_question_id %>"></div>
  <div class="fa fa-trash p-t-xs cjs_destroy_choice cursor-pointer" data-choice-id="<%= choice_id %>" data-profile-question-id="<%= profile_question_id %>"></div>
</li>