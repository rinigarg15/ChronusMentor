    if(wrapper == null)
      wrapper = ""
    class_name = class_name || ".cjs-members-select2";
    jQuery(wrapper + class_name ).each(function(){
      var select2Element = jQuery(this);
      select2Element.select2({
        multiple: oneToMany,
        id: function(object){
