              search: term,
              page: page,
              userIdsToIgnore: GroupUpdate.selectedUserIds
            };
          },
          results: function(data, page){
            var moreResultsAvailable = (page * parseInt("<%= SELECT2_PER_PAGE_LIMIT %>")) < data.total_count;
            return {
              results: data.users,
              more: moreResultsAvailable
            };
