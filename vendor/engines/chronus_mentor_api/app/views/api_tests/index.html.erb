<!-- We provide an infrastructure to scanner to crawl thrugh api links with proper methods -->
<table id="get_api" class="has-below-1">
  <tr><h3>Index and Show Links:</h3></tr>
  <% @get_api_links.each do |key, value| %>
    <tr><td><%= link_to key, value %></td></tr>
  <% end %>
</table>
<table id="post_api" class="has-below-1">
  <tr><h3>Create Links:</h3></tr>
  <% @post_api_links.each do |key, value| %>
    <tr><td><%= link_to key, value, :method => :post %></td></tr>
  <% end %>
</table>
<table id="put_api" class="has-below-1">
  <tr><h3>Update Links:</h3></tr>
  <% @put_api_links.each do |key, value| %>
    <tr><td><%= link_to key, value, :method => :put %></td></tr>
  <% end %>
</table>
<table id="delete_api" class="has-below-1">
  <tr><h3>Delete Links:</h3></tr>
  <% @delete_api_links.each do |key, value| %>
    <tr><td><%= link_to key, value, :method => :delete %></td></tr>
  <% end %>
</table>

